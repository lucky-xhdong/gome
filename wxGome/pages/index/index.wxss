.container {
  display: flex;
  flex-direction: column;
  width: 750rpx;
}

.banner-box,  .banner {
  width: 750rpx;
  height: 360rpx;
  position: relative;
}

/*搜索  */

.search-box {
  top: 20rpx;
  left: 40rpx;
  text-align: center;
  width: 670rpx;
  height: 60rpx;
  position: absolute;
}

.search-btn {
  width: 670rpx;
  height: 60rpx;
  font: 400 30rpx/65rpx 'Microsoft YaHei';
  background: #fff;
  text-align: left;
  color: #888;
  border-radius: 40rpx;
}

.search-btn icon {
  margin-right: 10rpx;
  line-height: 60rpx;
  vertical-align: -5%;
  margin-left: 40rpx;
}

.QR_button {
  position: absolute;
  top: 35rpx;
  right: 80rpx;
  width: 30rpx;
  height: 28rpx;
}

.QR_button image {
  width: 100%;
  height: 100%;
}

/*快捷入口*/

.enter-list {
  position: relative;
  width: 750rpx;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.enter {
  width: 710rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
}

.enter-box {
  padding: 10rpx;
  display: flex;
  align-items: center;
  background: #fff;
   flex: 1; 
}

.enter-box .navigator-hover {
  background: none;
  opacity: 1;
}

.index-tit {
   flex: 1; 
  text-align: center;
}
 .enter-desc{
  overflow: hidden;
  max-width: 150rpx;
  white-space:nowrap;

}

.index-tit image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
}

/*广告图  */
.ad-list {
  width: 750rpx;
   position:fixed;
   top: 0;
   left: 0;

}
.ad-list-bg{
   width: 750rpx;
background: rgba(0, 0, 0, 0.6);
   position:fixed;
   top: 0;
   left: 0;
  box-sizing:border-box;
}


.del {
  width: 65rpx;
  height: 65rpx;
  margin-top: 175rpx;
  margin-left: 625rpx;
  position: relative;
}

.cancel {
  width: 65rpx;
  height: 65rpx;
}

.ad-box {
  position: absolute;
   top: 240rpx;
 left: 60rpx;
  width: 630rpx;
  height: 660rpx; 
  z-index: 99;
}
.ad-box >view{
  width: 100%;
  height: 100%;
}
.ad-box image{
  width: 100%;
  height: 100%;
}
/*广告位  */

.activityList {
  position: relative;
  width: 750rpx;
  padding: 20rpx 0;
  display: flex;
  flex-direction: column;
}

.activity-box {
  width: 750rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.activity {
  width: 750rpx;
  height: auto;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.activity-url {
  flex: 1;
  text-align: center;
}

.activity-img {
  width: 100%;
}
